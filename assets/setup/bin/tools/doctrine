#!/bin/bash
trap "exit 1" ERR

PROJECT='__PROJECT__'

if [ "$PROJECT" = 'proxy-dia' ]; then
  exit 0
fi

if [ "$PROJECT" = 'proxy-carrefour' ]; then
  exit 0
fi

if [ "$PROJECT" = 'proxy-fontvella' ]; then
  exit 0
fi

print_usage() {
  printf "Usage: bin/doctrine [-g] \n"
}

bin/console doctrine:schema:validate -e test

