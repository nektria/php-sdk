{
    "name": "nektria/php-sdk",
    "type": "library",
    "description": "Nektria PHP SDK",
    "minimum-stability": "stable",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "Nektria\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Javier Gonzalez Cuadrado",
            "email": "xgc1986@gmail.com"
        }
    ],
    "require": {
        "ext-openssl": "*",
        "ext-redis": "*",
        "php": ">=8.1",
        "symfony/http-foundation": ">=v6.0",
        "symfony/http-kernel": ">=v6.0",
        "symfony/http-client-contracts": ">=v3.4",
        "symfony/messenger": ">=6.0",
        "symfony/mime": ">= 6.0",
        "doctrine/orm": ">=2.19",
        "symfony/framework-bundle": ">=6.4",
        "symfony/serializer": ">=6.4",
        "symfony/lock": ">=6.4",
        "symfony/amqp-messenger": ">=6.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "dev-master",
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-doctrine": "^1.0",
        "phpstan/phpstan-mockery": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpstan/phpstan-strict-rules": "^1.0",
        "phpstan/phpstan-symfony": "^1.0",
        "phpunit/phpunit": "^11.0",
        "roave/security-advisories": "dev-master",
        "shipmonk/phpstan-rules": "^3.0",
        "squizlabs/php_codesniffer": "^3.0",
        "symfony/browser-kit": "^v7.0",
        "symfony/var-dumper": "^v7.0"
    }
}
