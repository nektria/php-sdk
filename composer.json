{
    "name": "nektria/php-sdk",
    "type": "library",
    "description": "Nektria PHP SDK - 1.0.391",
    "minimum-stability": "stable",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "Nektria\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Javier Gonzalez Cuadrado",
            "email": "xavi@nektria.com"
        }
    ],
    "require": {
        "php": ">=8.3 <8.4",
        "ext-amqp": "*",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-redis": "*",
        "ext-sodium": "*",
        "ext-zend-opcache": "*",
        "doctrine/common": "^3.0",
        "doctrine/dbal": "^3.0",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^3.0",
        "mk-j/php_xlsxwriter": "^0.39",
        "symfony/amqp-messenger": "^7.0",
        "symfony/dotenv": "^7.0",
        "symfony/http-client": "^7.0",
        "symfony/lock": "^7.0",
        "symfony/mercure-bundle": "^0.3",
        "symfony/messenger": "^7.0",
        "symfony/mime": "^v7.0",
        "symfony/monolog-bundle": "^v3.0",
        "symfony/process": "^7.0",
        "symfony/property-access": "^7.0",
        "symfony/runtime": "^7.0",
        "symfony/serializer": "^7.0",
        "symfony/translation-contracts": "^3.0",
        "symfony/yaml": "^7.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "dev-master",
        "ergebnis/phpstan-rules": "^2.0",
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-doctrine": "^1.0",
        "phpstan/phpstan-mockery": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpstan/phpstan-strict-rules": "^1.0",
        "phpstan/phpstan-symfony": "^1.0",
        "phpunit/phpunit": "^11.0",
        "roave/security-advisories": "dev-master",
        "shipmonk/phpstan-rules": "^3.0",
        "spaze/phpstan-disallowed-calls": "^3.4",
        "squizlabs/php_codesniffer": "^3.0",
        "symfony/browser-kit": "^7.0",
        "symfony/var-dumper": "^7.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/runtime": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        }
    }
}